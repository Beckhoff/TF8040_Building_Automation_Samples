TYPE ST_BA_GeneralSettings :
STRUCT
	fFrostProtectionSetpoint			: REAL;  // frost protection setpoint. for eg. Heating circutis in protection mode
	fTWth								: REAL;  // present value of outside temperature

	fTWthLowLimit						: REAL;  // below this value all frostprotection funcktions in HVAC-Plants will be enabled
	fTWthDamped							: REAL;  // damped outside temperature

	bTWthLowLimit						: BOOL;  // is TRUE when the outside temperature is below the value from fTWthLmtCrit 
	bTWthDampedLowLimit					: BOOL;  // is TRUE when the damped outside temperature is below the value from fTWthLmtCrit 

	nDummy								: ARRAY [1..6] OF BYTE;
END_STRUCT
