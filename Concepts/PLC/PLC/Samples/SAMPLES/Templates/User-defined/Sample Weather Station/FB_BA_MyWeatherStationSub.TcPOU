    <Method Name="GetData" Id="{09db79f0-a1cc-440f-b09b-f8092fbaa0a4}" FolderPath="Management\">
      <Declaration><![CDATA[// Returns subscription-data (For storing read data).
METHOD GetData
VAR_OUTPUT
	pData			: PVOID;				// Pointer to subscribed data.
	nSize			: DINT;					// Size of subscribed data.
END_VAR]]></Declaration>
      <Implementation>
        <ST><![CDATA[pData := ADR(stData);
nSize := SIZEOF(stData);]]></ST>
      </Implementation>
    </Method>
    <Method Name="OnValidateSubscription" Id="{b4a3d99f-a1de-4c68-9ca0-cad4df264726}" FolderPath="[Protected] Events\">
      <Declaration><![CDATA[METHOD PROTECTED OnValidateSubscription : BOOL
VAR_INPUT
	iPublisher			: I_BA_Publisher;
END_VAR
VAR
	_iSubWthSt			: I_BA_MyWeatherStation;
END_VAR]]></Declaration>
      <Implementation>
        <ST><![CDATA[// Cast interface pointer (Used for a local subscription):
OnValidateSubscription := __QUERYINTERFACE(iPublisher, _iSubWthSt);]]></ST>
      </Implementation>
