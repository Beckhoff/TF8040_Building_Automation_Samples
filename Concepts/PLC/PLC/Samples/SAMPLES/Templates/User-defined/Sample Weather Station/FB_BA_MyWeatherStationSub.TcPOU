<?xml version="1.0" encoding="utf-8"?>
<TcPlcObject Version="1.1.0.1" ProductVersion="3.1.4024.9">
  <POU Name="FB_BA_MyWeatherStationSub" Id="{17417506-e33f-4972-b839-5e8a66f2f355}" SpecialFunc="None">
    <Declaration><![CDATA[// [Sample]
// Subscriber to a user specific weather station.
FUNCTION_BLOCK FB_BA_MyWeatherStationSub EXTENDS FB_BA_Subscriber
VAR_OUTPUT
	fWeatherTemperature	: REAL;
	fHumidity			: REAL;
	fBrightness			: REAL;
	
	bDisturb			: BOOL;
END_VAR
VAR
	{region 'General'}
		stData			: ST_BA_MyWeatherData;
	{endregion}
END_VAR]]></Declaration>
    <Implementation>
      <ST><![CDATA[SUPER^();

// Update outputs of most important variables:
fWeatherTemperature := stData.fWeatherTemperature;
fHumidity := stData.fHumidity;
fBrightness := stData.fBrightness;

// Indicate:
// - A physical error that is really occured in the remote weather station
// - A reference error (E.g. broken ADS connection)
bDisturb := (stData.bDisturb) OR (Error);]]></ST>
    </Implementation>
